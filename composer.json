{
  "name": "heimrichhannot/contao-tree-bundle",
  "description": "A contao bundle to create a tree structure like an organization chart in the contao backend and output it in the frontend.",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Heimrich & Hannot GmbH",
      "email": "digitales@heimrich-hannot.de"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "contao/core-bundle": "^4.4",
    "heimrichhannot/contao-utils-bundle": "^2.171",
    "symfony/event-dispatcher-contracts": "^1.0 || ^2.0 || ^3.0"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\TreeBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\TreeBundle\\ContaoManager\\Plugin"
  }
}